/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
	Not(in=a, out=nota);
    Not(in=b, out=notb);
	Nand(a=a, b=notb, out=x1);
	Nand(a=nota, b=b, out=x2);
	Nand(a=x1,b=x2,out=out);
}