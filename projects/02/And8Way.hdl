CHIP And8Way {
    IN a,b,c,d,e,f,g,h;
    OUT out;

    PARTS:
    And4Way(a=a, b=b, c=c, d=d, out =a1);
	And4Way(a=e, b=f, c=g, d=h, out =a2);
	And(a=a1, b=a2, out =out);
}
