CHIP And16Way {
    IN in[16];
    OUT out;

    PARTS:
    And8Way(a=in[0], b=in[1], c=in[2], d=in[3], a=in[4], b=in[5], c=in[6], d=in[7], out =a1);
	And8Way(a=in[8], b=in[9], c=in[10], d=in[11], a=in[12], b=in[13], c=in[14], d=in[15], out =a2);
	And(a=a1, b=a2, out =out);
}
